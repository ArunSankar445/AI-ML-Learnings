   "source": [
    "def fib(no_of_term):\n",
    "    first = 0\n",
    "    second = 1\n",
    "\n",
    "    for i in range (no_of_term):\n",
    "        yield first\n",
    "        new = first + second\n",
    "        first = second\n",
    "        second = new\n",
    "    \n"
   ]
  },
